{
    "name": "vitexsoftware/abraflexi-invoice-enhancer",
    "description": "Invoice Enhancer for AbraFlexi",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Cybervitexus",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "vitexsoftware/ease-bootstrap5-widgets-abraflexi": "*",
        "vitexsoftware/ease-bootstrap5-widgets": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.87",
        "ergebnis/composer-normalize": "^2.48",
        "ergebnis/php-cs-fixer-config": "^6.55",
        "phpstan/phpstan-phpunit": "2.0.7"
    },
    "autoload": {
        "psr-4": {
            "AbraFlexi\\Enhancer\\": "src/Enhancer/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
