{
  "name": "deb/abraflexi-relationship",
  "description": "Relationship with customer report generator for FlexiBee",
  "type": "project",
  "version": "0.1",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "config": {
    "vendor-dir": "/var/lib/abraflexi-relationship"
  },
  "require": {
    "deb/ease-core": "*",
    "deb/abraflexi-bricks": "*",
    "deb/ease-bootstrap4-widgets": "*",
    "deb/ease-bootstrap4-widgets-abraflexi": "*"
  },
  "autoload": {
    "psr-4": {
      "AbraFlexi\\Relationship\\": "./Relationship/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseHtml/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexi/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseBricks/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB4/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexiBricks/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB4Widgets/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB4WidgetsAbraFlexi/"
    }
  ]
}
