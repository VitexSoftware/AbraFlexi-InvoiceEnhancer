{
  "name": "deb/abraflexi-enhancer",
  "description": "Enhancer with customer report generator for FlexiBee",
  "type": "project",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "config": {
    "vendor-dir": "/var/lib/abraflexi-enhancer"
  },
  "require": {
    "deb/ease-core": "*",
    "deb/abraflexi-bricks": "*",
    "deb/ease-bootstrap5-widgets": "*",
    "deb/ease-bootstrap5-widgets-abraflexi": "*"
  },
  "autoload": {
    "psr-4": {
      "AbraFlexi\\Enhancer\\": "./Enhancer/",
      "AbraFlexi\\Enhancer\\ui\\": "./Enhancer/ui/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseHtml/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexi/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseBricks/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB5/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexiBricks/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB5Widgets/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB5WidgetsAbraFlexi/"
    }
  ]
}
